{
  "filters": {
    "comments": true
  },
  "rules": {
    "eslint": {
      "configFile": "./config/markdown.eslintrc.js"
    },
    "preset-japanese": true,
    "ja-no-mixed-period": true,
    "no-start-duplicated-conjunction": {
      "interval": 2
    },
    "prh": {
      "rulePaths": [
        "prh.yml"
      ]
    }
  }
}
